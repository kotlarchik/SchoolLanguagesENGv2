<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kotlarchik.controllers.ControllerLogIn">
   <children>
      <GridPane layoutX="31.0" layoutY="35.0" prefHeight="122.0" prefWidth="338.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="164.0" minWidth="10.0" prefWidth="120.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="268.0" minWidth="10.0" prefWidth="218.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER_RIGHT" text="Login:">
               <font>
                  <Font name="Comic Sans MS Bold" size="25.0" />
               </font>
               <GridPane.margin>
                  <Insets left="45.0" />
               </GridPane.margin>
            </Label>
            <Label alignment="CENTER_RIGHT" text="Password:" GridPane.rowIndex="1">
               <font>
                  <Font name="Comic Sans MS Bold" size="25.0" />
               </font>
            </Label>
            <TextField fx:id="txtLogin" GridPane.columnIndex="1" />
            <TextField fx:id="txtPass" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="buttonSign" layoutX="155.0" layoutY="231.0" mnemonicParsing="false" onAction="#pressSign" text="SignIn">
         <font>
            <Font name="Comic Sans MS Bold" size="20.0" />
         </font>
      </Button>
      <Label fx:id="txtStatus" alignment="CENTER" layoutX="31.0" layoutY="177.0" prefHeight="36.0" prefWidth="338.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
